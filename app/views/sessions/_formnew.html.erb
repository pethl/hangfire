
  <div class="well well-small">
  <%= form_for(:session, url: login_path) do |f| %>
    
     <%= f.label :email %>
     <%= f.text_field :email %><br>
     
     <%= f.label :password %>
     <%= f.password_field :password %>

     <%= f.submit "Sign in", class: "btn btn-large btn-success" %>
   <% end %>

</div>
