<h1>Listing eventplates</h1>

<table>
  <thead>
    <tr>
      <th>Eventplanner</th>
      <th>Plate</th>
      <th>Covers</th>
      <th>Over ride price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @eventplates.each do |eventplate| %>
      <tr>
        <td><%= eventplate.eventplanner_id %></td>
        <td><%= eventplate.plate_id %></td>
        <td><%= eventplate.covers %></td>
        <td><%= eventplate.over_ride_price %></td>
        <td><%= link_to 'Show', eventplate %></td>
        <td><%= link_to 'Edit', edit_eventplate_path(eventplate) %></td>
        <td><%= link_to 'Destroy', eventplate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Eventplate', new_eventplate_path %>
